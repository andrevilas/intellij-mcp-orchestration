{
  "name": "mcp-console-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.5.2",
  "scripts": {
    "dev": "node ./scripts/run-dev.mjs",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest --run",
    "lint": "eslint src",
    "report:bundle": "node ./scripts/report-bundle.mjs",
    "build:bundle-report": "node ./scripts/run-performance-build.mjs",
    "analyze:bundle": "pnpm run build:bundle-report",
    "lighthouse:ci": "node ./scripts/run-lighthouse-ci.mjs",
    "report:performance": "pnpm run build:bundle-report && pnpm run lighthouse:ci"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "clsx": "^2.1.1",
    "diff": "^8.0.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.52.1",
    "reactflow": "^11.10.0",
    "recharts": "^2.12.7"
  },
  "devDependencies": {
    "@lhci/cli": "^0.14.0",
    "@puppeteer/browsers": "^2.10.12",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitejs/plugin-react": "^4.3.1",
    "cssnano": "^6.0.0",
    "eslint": "^8.57.1",
    "globals": "^15.9.0",
    "jsdom": "^24.1.0",
    "msw": "^2.6.3",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass-embedded": "^1.77.5",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vitest": "^1.6.0"
  },
  "engines": {
    "node": ">=18.18.0",
    "pnpm": ">=10.5.2"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
